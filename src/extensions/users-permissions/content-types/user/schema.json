{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "articulos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::articulo.articulo",
      "mappedBy": "registrado"
    },
    "mantenimientos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::mantenimiento.mantenimiento",
      "mappedBy": "encargado"
    },
    "cambios": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::cambio.cambio",
      "mappedBy": "responsable"
    },
    "area_permisos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::area-permiso.area-permiso",
      "mappedBy": "user"
    },
    "cedula": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "nombres": {
      "type": "string",
      "required": true
    },
    "apellidos": {
      "type": "string",
      "required": true
    },
    "articulos_asignados": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::articulo.articulo",
      "mappedBy": "responsable"
    }
  }
}
